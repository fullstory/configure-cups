#!/usr/bin/make -f

include /usr/share/cdbs/1/rules/debhelper.mk

install/configure-cups::
	mkdir -p debian/configure-cups/usr/share/pixmaps
	convert icons/hicolor/32x32/apps/sidux-cups.png \
		debian/configure-cups/usr/share/pixmaps/configure-cups.xpm
	convert icons/hicolor/16x16/apps/sidux-cups.png \
		debian/configure-cups/usr/share/pixmaps/configure-cups-16.xpm
